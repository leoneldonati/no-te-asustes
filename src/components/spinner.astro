---
import { IconLoader } from "@tabler/icons-react";
---

<IconLoader id="spinner" />

<style>
  #spinner {
    animation: spin 1s 1s infinite  cubic-bezier(0.785, 0.135, 0.15, 0.86);
  }
  @keyframes spin {
    from {
      transform: rotate(0);
    }
    to {
      transform: rotate(360deg);
    }
  }
</style>